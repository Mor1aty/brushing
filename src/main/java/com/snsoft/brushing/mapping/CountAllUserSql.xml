<mapper namespace="com.snsoft.brushing.dao.CountAllUserDao">
	<!-- 获取所有用户 -->
	<select id = "getAllUser" resultType="com.snsoft.brushing.pojo.User">
		SELECT
			u_account,
			u_name,
			u_identity,
			u_star
		FROM
			t_user;
